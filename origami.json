{
	"description": "FT-branded crosswords",
	"origamiType": "module",
	"origamiVersion": 1,
	"support": "https://github.com/ftlabs/o-crossword/issues",
	"supportStatus": "experimental",
	"ci": {
		"circle": "https://circleci.com/api/v1/project/ftlabs/o-crossword"
	},
	"browserFeatures": {
		"required": [
			"fetch",
			"Array.from",
			"Element.prototype.classList",
			"requestAnimationFrame",
			"Node.prototype.contains",
			"Array.prototype.find",
			"Array.prototype.filter"
		]
	},
	"demosDefaults": {
		"js": "demos/src/demo.js",
		"sass": "demos/src/demo.scss"
	},
	"demos": [
		{
			"name": "basic",
			"template": "demos/src/basic.mustache",
			"expanded": true,
			"description": ""
		},
		{
			"name": "answered",
			"template": "demos/src/answered.mustache",
			"expanded": false,
			"description": ""
		},
		{
			"name": "pre-compiled",
			"template": "demos/src/pre-compiled.mustache",
			"expanded": false,
			"description": ""
		},
		{
			"name": "force-compact",
			"template": "demos/src/force-compact.mustache",
			"expanded": false,
			"description": "Force a compact view even on desktops."
		}
	]
}
